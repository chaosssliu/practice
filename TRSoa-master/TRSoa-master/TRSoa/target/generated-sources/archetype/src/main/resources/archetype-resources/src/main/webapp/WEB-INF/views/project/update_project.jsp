#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<%@include file="/WEB-INF/views/includes/header.jsp" %><%@include file="/WEB-INF/views/includes/navi.jsp" %><div class="col-lg-12">    <div class="panel panel-default">        <div class="panel-heading">            <h4>Project List</h4>        </div>        <div class="panel-body">            <table class="table table-condensed table-hover"                   style="border-collapse: collapse;">                <thead>                <tr>                    <th>&nbsp;</th>                    <th>Project. Name</th>                    <th>Description</th>                    <th>Start Date</th>                    <th>Location</th>                    <th>Status</th>                </tr>                </thead>                <tbody>                <c:forEach items="${symbol_dollar}{projList}" var="proj">                    <tr data-toggle="collapse" data-target="${symbol_pound}dept_${symbol_dollar}{proj.id}"                        class="accordion-toggle">                        <td><button class="btn btn-default btn-md">                            <span class="glyphicon glyphicon-pencil"></span>                        </button></td>                        <td>${symbol_dollar}{proj.name}</td>                        <td>${symbol_dollar}{proj.desc}</td>                        <td><fmt:formatDate value="${symbol_dollar}{proj.startDate}" pattern="MM/dd/yyyy" /></td>                        <td>${symbol_dollar}{proj.locationDetail.customAddress}</td>                        <td>${symbol_dollar}{proj.statusStr}</td>                    </tr>                </c:forEach>                </tbody>            </table>        </div>    </div></div><%@include file="/WEB-INF/views/includes/footer.jsp" %>